{{ section.settings.text }}
<br/>
{%- for block in section.blocks -%}
    {%- case block.type -%}
        {%- when 'announcement' -%}
            {{block.settings.qwer}}
            <br/>
        {%- else -%}
    {%- endcase -%}
{%- endfor -%}

{% schema %}
{
    "name": "Testing",
    "tag": "section",
    "settings": [
        {
            "type": "text",
            "id": "text",
            "default": "Welcome to our store 111",
            "label": "section title"
        }
    ],
    "blocks": [
        {
            "name": "Announcement",
            "type": "announcement",
            "settings": [
                {
                    "type": "text",
                    "id": "qwer",
                    "label": "block title",
                    "info": "More info about Block",
                    "placeholder": "A placeholder value for the input"
                }
            ]
        }
    ]
}
{% endschema %}