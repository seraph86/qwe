{{ section.settings.title }}
<br/>
{%- for block in section.blocks -%}
    {%- case block.type -%}
        {%- when 'announcement' -%}
            {{block.settings.heading}}
            <br/>
            {{block.settings.title}}
            <br/>
            {{block.settings.text}}
        {%- else -%}
    {%- endcase -%}
{%- endfor -%}

{% schema %}
{
    "name": "Testing",
    "tag": "section",
    "settings": [
        {
            "type": "text",
            "id": "qwe",
            "label": "B"
        },
        {
            "type": "text",
            "id": "title",
            "label": "A"
        }
    ],
    "blocks": [
        {
            "name": "Announcement",
            "type": "announcement",
            "settings": [
                {
                    "type": "text",
                    "id": "heading",
                    "label": "1"
                },
                {
                    "type": "text",
                    "id": "title",
                    "label": "2"
                },
                {
                    "type": "text",
                    "id": "text",
                    "label": "3"
                }
            ]
        }
    ],
    "preset":[
        {
            "name": "Testing",
            "settings": {
                "title": "Yo"
            }
        }
    ]
}
{% endschema %}